{
	"homepage": "https://d-cide.me",
	"name": "d-cide",
	"version": "1.78.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.11.4",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.58",
		"@reduxjs/toolkit": "^1.6.0",
		"crypto-js": "^4.0.0",
		"eslint-plugin-unused-imports": "^1.1.1",
		"framer-motion": "^4.1.17",
		"react": "^17.0.2",
		"react-device-detect": "^1.17.0",
		"react-dom": "^17.0.2",
		"react-ga": "^3.3.0",
		"react-redux": "^7.2.4",
		"react-scripts": "^5.0.1",
		"react-swipeable-views": "^0.14.0",
		"recharts": "^2.0.9",
		"redux": "^4.1.0",
		"redux-offline": "^2.0.0",
		"redux-thunk": "^2.3.0",
		"rxjs": "^7.1.0",
		"yargs-parser": "^20.2.7"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --watchAll=false --forceExit & npm run cy:run",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"preinstall": "npx npm-force-resolutions",
		"predeploy": "git checkout master && git pull origin master && git merge develop && git branch -D develop || git checkout -b develop",
		"deploy": "cypress run && react-scripts test --watchAll=false --forceExit && npm version minor -f && git push origin master && git branch -D develop || git checkout -b develop",
		"check-types": "tsc --noemit -p tsconfig.json",
		"update-packages": "npm update",
		"lint": "eslint --max-warnings=0",
		"lint:fix": "yarn lint --fix",
		"lint:all": "yarn lint \"**/*.+(js|jsx|ts|tsx)\"",
		"lint:fix:all": "yarn lint:fix \"**/*.+(js|jsx|ts|tsx)\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/axe-core": "^2.0.7",
		"@types/crypto-js": "^4.0.1",
		"@types/cypress-axe": "^0.11.0",
		"@types/jest": "^26.0.23",
		"@types/jwt-decode": "^3.1.0",
		"@types/react": "^17.0.11",
		"@types/react-dom": "^17.0.7",
		"@types/react-redux": "^7.1.16",
		"@types/react-router-dom": "^5.1.7",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/recharts": "^2.0.0",
		"@types/redux-mock-store": "^1.0.2",
		"@types/webpack-env": "^1.16.0",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.26.1",
		"babel-eslint": "^10.1.0",
		"chai-colors": "^1.0.1",
		"cypress": "^7.5.0",
		"eslint": "^7.28.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.7.2",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"gh-pages": "^3.2.0",
		"git-branch-is": "^4.0.0",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"prettier": "2.3.1",
		"redux-mock-store": "^1.5.4",
		"typescript": "^4.3.2"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"<rootDir>/build/",
			"<rootDir>/node_modules/"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{html,css,less,ejs,json}": [
			"prettier --write"
		]
	}
}
